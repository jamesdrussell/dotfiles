{
  "agent": {
    "default_model": {
      "provider": "google",
      "model": "gemini-2.5-pro-preview-03-25"
    },
    "version": "2"
  },
  "theme": "Catppuccin Mocha",
  "buffer_font_family": "JetBrains Mono NL",
  "buffer_font_size": 16,
  "vim_mode": false,
  "cursor_blink": true,
  "ensure_final_newline_on_save": false,
  "format_on_save": "off",
  "remove_trailing_whitespace_on_save": true,
  "hover_popover_enabled": false,
  "gutter": {
    "code_actions": false
  },
  "tab_size": 2,
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "auto_install_extensions": {
    "html": true,
    "just": true,
    "dockerfile": true,
    "toml": true,
    "terraform": true,
    "git-firefly": true,
    "lua": true,
    "docker-compose": true,
    "meson": true,
    "nix": true,
    "pylsp": true,
    "sql": true,
    "swift": true,
    "proto": true,
    "catppuccin": true
  },
  "enable_language_server": true,
  "show_wrap_guides": true,
  "wrap_guides": [80]
}